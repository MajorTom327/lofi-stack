    "isbot": "^4.1.0",
    "lucide-react": "^0.325.0",
    "luxon": "^3.4.4",
    "pino": "^8.18.0",
    "rambda": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
