{
  "name": "lofi-stack",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "test": "vitest -c ./test/vitest.config.ts --ui --coverage",
    "test:ci": "vitest -c ./test/vitest.config.ts run",
    "prettier": "prettier --write ./app",
    "create:page": "node ./scripts/createPage.mjs",
    "lint": "eslint \"app/**/*.+(ts|tsx)\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "prettier app --write",
    "format-code": "npm run prettier:fix & npm run lint:fix",
    "prepare": "husky install",
    "db:update": "concurrently \"npm:db:push\" \"npm:db:gen\"",
    "db:push": "prisma db push --force-reset",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@remix-run/css-bundle": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@types/crypto-js": "^4.1.2",
    "ahooks": "^3.7.8",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "crypto-js": "^4.1.1",
    "isbot": "^3.6.8",
    "lucide-react": "^0.285.0",
    "luxon": "^3.4.3",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "prisma": "^5.3.1",
    "ramda": "^0.29.0",
    "ramda-adjunct": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.5.1",
    "remix-auth-form": "^1.3.0",
    "remix-hook-form": "^2.0.1",
    "remix-utils": "^6.6.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.0.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/luxon": "^3.3.2",
    "@types/ramda": "^0.29.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "autoprefixer": "^10.4.16",
    "chroma-js": "^2.4.2",
    "concurrently": "^8.2.1",
    "eslint": "^8.38.0",
    "happy-dom": "^12.1.5",
    "husky": "^8.0.3",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5",
    "vitest-mock-extended": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
